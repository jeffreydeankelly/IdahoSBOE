<!-- The base derivation for ICE-based browsers -->
<styleSheet browsers="ice">

  <style name="list">
    <!-- ice doesn't seem to respect list-style-position,
         so adding padding as workaround -->
    <property name="padding">0px 0px 0px 12px</property>
  </style>
</styleSheet>

<!-- The base derivation for Gecko-based browsers -->
<styleSheet browsers="gecko">

  <!-- Dunno why, but Gecko makes asterisks look very thin -->
  <style selector=".AFRequiredIconStyle">
    <property name="font-weight">bold</property>
  </style>

  <style selector=".p_OraContentFooterRule">
    <includeStyle name="AFDarkBackground"/>
  </style>    

  <style selector=".OraSortableHeaderBorder">
     <property name="border-style">inset</property>
  </style>

  <style selector="af|selectInputDate::content">
    <property name="line-height">1.8</property>
  </style>
  
  <style selector="af|menuButtons::icon-style">
    <property name="vertical-align">bottom</property>
  </style>
  
  <style selector=".p_OraHideSkipNaviText">
    <property name="font-size">0pt</property>
    <property name="margin-top">0px</property>
    <property name="margin-left">0px</property>
  </style>

  <!-- IE 6 does not yet seem to support min-width correctly -->
  <style selector="af|showDetailFrame::sub-menu-container">
    <property name="min-width">120px</property>
  </style>

  <style selector="af|panelCustomizable::sub-menu-container">
    <property name="min-width">120px</property>
  </style>

</styleSheet>

<styleSheet platforms="macos" browsers="ie" versions="5">
  <!-- Override on mac as it doesn't show up.-->
  <style name="MediumExtraAccentForeground">
    <property name="color"/>
  </style>

  <style selector="af|selectinputDate::content">
    <property name="line-height">1.8</property>
  </style>

  <!-- Style for alignment in table cells -->
  <style name="AFTableCellDataVerticalAlign">
    <property name="vertical-align"/>
  </style>


</styleSheet>

<!-- CJK style sheet for all -->
<styleSheet locales="ja zh ko">

  <!--
    We don't specify a font family for CJK languages, since most browsers
    (Netscape, ICE, IE) seem to have problems displaying CJK text with
    our default font-family list (Arial, Helvetica, etc...).  We're better 
    off letting the font-family default.  (However - that just causes
    still more problems on Windows Netscape 4 - so we've got more overrides
    below for that specific case.
  -->
  <style name="AFDefaultFontFamily">
    <property name="font-family"/>
  </style>

  <!-- In order for CJK glyphs rendered on the server to be
       appear readable, we need to crank up the font size and
       turn on antialiasing -->
  <style name="DefaultServerFont">
    <property name="font-size">+1pt</property>
    <property name="text-antialias">true</property>
  </style>

</styleSheet>

<!-- Style sheet just for Japanese-specific styles -->
<styleSheet locales="ja">

  <style name="DefaultServerFontFamily">
    <property name="font-family">Albany WT J, Dialog</property>
  </style>

</styleSheet>

<!-- Style sheet just for Korean-specific styles -->
